{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"importsNotUsedAsValues": "error",
		"paths": {
			"#pages/*": [
				"src/pages/*"
			],
			"#layouts/*": [
				"src/layouts/*"
			],
			"#components/*": [
				"src/components/*"
			],
			"#images/*": [
				"src/assets/images/*"
			],
			"#styles/*": [
				"src/assets/styles/*"
			]
		}
	},
}
